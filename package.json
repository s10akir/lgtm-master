{
  "name": "app",
  "private": "true",
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "esbuild": "^0.19.5",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=/assets",
